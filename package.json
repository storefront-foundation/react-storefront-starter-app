{
  "name": "react-storefront-starter-app",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development node server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "build": "rm -rf .next && next build && npm run xdn",
    "xdn": "node node_modules/xdn-next/build.js",
    "test": "jest",
    "deploy": "preact=false npm run build && xdn deploy",
    "rsf:link": "yalc add react-storefront react-storefront-analytics react-storefront-amp xdn-router xdn-next && npm i",
    "prettier": "prettier --write \"**/*.js\" \"!{node_modules,.next,.yalc}/**\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.35",
    "@material-ui/styles": "4.5.2",
    "@svgr/webpack": "^4.3.3",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^1.0.4",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "lorem-ipsum": "^2.0.3",
    "module-alias": "^2.2.1",
    "next": "^9.2.0",
    "path-to-regexp": "^3.1.0",
    "preact": "10.1.0",
    "preact-render-to-string": "^5.1.2",
    "preact-ssr-prepass": "^1.0.0",
    "qs": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-lazy-hydration": "0.0.8",
    "react-storefront": "^7.3.0",
    "react-storefront-amp": "^1.1.0",
    "react-storefront-analytics": "^1.0.0",
    "react-visibility-sensor": "^5.1.1",
    "typeface-roboto": "0.0.75",
    "xdn-next": "^0.5.0",
    "xdn-router": "^0.5.0"
  },
  "devDependencies": {
    "@babel/helper-function-name": "^7.8.3",
    "@babel/helper-split-export-declaration": "^7.8.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-imports": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^2.1.2",
    "jsdom": "^15.2.1",
    "prettier": "^1.19.1",
    "rewire": "^4.0.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-clear-require-cache-plugin": "^0.0.3",
    "xdn": "^0.0.6"
  },
  "lint-staged": {
    "*.js": [
      "yalc remove --all",
      "npm run prettier",
      "git add"
    ]
  }
}
