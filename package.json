{
  "name": "react-storefront-starter-app",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "homepage": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "build": "next build",
    "test": "jest --passWithNoTests",
    "deploy": "npm run build && xdn deploy",
    "rsf:link": "yalc add react-storefront",
    "prettier": "prettier --write \"**/*.js\" \"!{node_modules,.next,.yalc}/**\"",
    "storybook": "start-storybook -c ./.storybook",
    "storybook-docs": "mkdir -p .storybook/build && PRESERVE_MARKDOWN=true NODE_ENV=docs babel-node .storybook/createMetadata d.js",
    "build-storybook": "build-storybook -c .storybook -o .storybook/build && npm run storybook-docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/styles": "4.11.4",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.0.4",
    "cross-env": "^7.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "lorem-ipsum": "^2.0.3",
    "module-alias": "^2.2.1",
    "next": "^10.2.3",
    "preact": "10.5.13",
    "preact-render-to-string": "^5.1.19",
    "preact-ssr-prepass": "^1.1.3",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-storefront": "^8.18.3",
    "react-visibility-sensor": "^5.1.1",
    "react-swipeable-views": "0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@next/plugin-storybook": "^11.0.1",
    "@storybook/addon-a11y": "^6.3.0",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-jest": "^6.3.0",
    "@storybook/addon-measure": "^1.3.1",
    "@storybook/addon-toolbars": "^6.3.0",
    "@storybook/addon-viewport": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "dotenv": "^10.0.0",
    "jest": "^27.0.5",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "storybook-addon-pseudo-states": "^1.0.0"
  },
  "lint-staged": {
    "*.js": [
      "yalc remove --all",
      "npm run prettier",
      "git add"
    ]
  }
}
