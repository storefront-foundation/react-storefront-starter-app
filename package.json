{
  "scripts": {
    "edgio": "edgio",
    "edgio:dev": "edgio dev",
    "edgio:build": "edgio build",
    "edgio:deploy": "edgio deploy",
    "next": "next",
    "start": "cross-env SERVICE_WORKER=true edgio dev --cache",
    "build": "edgio build",
    "prod": "edgio run --production",
    "test": "jest",
    "deploy": "edgio deploy react-storefront",
    "prettier": "prettier --write \"**/*.js\" \"!{node_modules,.edgio,.next,.yalc}/**\"",
    "rsf:link": "rm -rf node_modules/react-storefront node_modules/react-storefront-analytics node_modules/react-storefront-amp && yalc add react-storefront react-storefront-analytics react-storefront-amp && npm i --prefer-offline",
    "edgio:link": "rm -rf node_modules/@edgio/core node_modules/@edgio/next && yalc add @edgio/next @edgio/core @edgio/cli @edgio/prefetch",
    "rsf:update": "npm i --save react-storefront@latest react-storefront-amp@latest react-storefront-analytics@latest",
    "edgio:update": "npm i --save @edgio/core@latest @edgio/next@latest @edgio/cli@latest @edgio/prefetch@latest",
    "reinstall": "rm -rf node_modules && npm i --prefer-offline"
  },
  "name": "react-storefront-starter-app",
  "version": "1.0.0",
  "private": true,
